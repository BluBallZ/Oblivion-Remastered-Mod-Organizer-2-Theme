/* Progress Bars (Downloads) #QProgressBar */
QProgressBar {
  background-color: #9e9978;
  color: #000;
  border-color: #bcbcbc;
  text-align: center;
  border-style: solid;
  border-width: 1px;
  margin: 0px;
}

QProgressBar::chunk {
  background: #06b025;
}

DownloadListView[downloadView=standard]::item {
  padding: 16px;
}

DownloadListView[downloadView=compact]::item {
  padding: 4px;
}

/* Sliders #QSlider */
QSlider::groove:horizontal {
  background-color: #9e9978;
  border: none;
  height: 8px;
  margin: 2px 0;
}

QSlider::handle:horizontal {
  width: 0.5em;
  height: 2em;
  margin: -7px 0;
  subcontrol-origin: margin;
  color: #dddddd;
  background-color: #9e9978;
  border-color: #9e9978;
  border-style: solid;
  border-width: 1px;
}

QSlider::handle:horizontal:hover,
QSlider::handle:horizontal:pressed,
QSlider::handle:horizontal:focus:pressed {
  background-color: #747058;
  border-color: #DED7A8;
}

QSlider::handle:horizontal:focus {
  background-color: #9e9978;
  border-color: #DED7A8;
}

QSlider::handle:horizontal:disabled {
  background-color: #9e9978;
  border-color: #747058;
}

QSlider::handle:vertical {
  color: #dddddd;
  background-color: #9e9978;
  border-color: #9e9978;
  border-style: solid;
  border-width: 1px;
}

QSlider::handle:vertical:hover,
QSlider::handle:vertical:pressed,
QSlider::handle:vertical:focus:pressed {
  background-color: #747058;
  border-color: #DED7A8;
}

QSlider::handle:vertical:focus {
  background-color: #9e9978;
  border-color: #DED7A8;
}

QSlider::handle:vertical:disabled {
  background-color: #9e9978;
  border-color: #747058;
}